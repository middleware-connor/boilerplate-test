{
  "name": "@torderdev/boilerplate-test",
  "version": "0.1.1",
  "description": "미들웨어 NestJS Boilerplate",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint ./src",
    "build": "npm run ts-patch && tsc",
    "generate:sdk": "rm -rf ./src/api && rm -rf ./packages/api && NODE_ENV=local npx nestia sdk --config nestia.config.ts --project tsconfig.json",
    "build:sdk": "npm run generate:sdk && node ./scripts/prepare-sdk-build.js > ./packages/api/package.json && cp ./tsconfig.sdk.json ./packages/api/tsconfig.json && cd ./packages/api && npm run build",
    "build:swagger": "NODE_ENV=local npx nestia swagger",
    "build:compodoc": "npx compodoc -p tsconfig.doc.json -n \"NestJS Documentation\" -d ./docs/compodoc",
    "start": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register ./dist/src/main.js",
    "start:local": "NODE_ENV=local nodemon --watch dist --exec \"npm run start\"",
    "start:dev": "NODE_ENV=development npm run start",
    "start:prod": "NODE_ENV=production npm run start",
    "ts-patch": "ts-patch install && typia patch",
    "redoc": "docker run -it --rm -p 8000:80 -e SPEC_URL=swagger.json -e 'REDOC_OPTIONS=sort-tags-alphabetically=\"true\" min-character-length-to-init-search=1' -v $(pwd)/docs/swagger.json:/usr/share/nginx/html/swagger.json redocly/redoc",
    "publish:sdk": "npm run build:sdk && cd ./packages/api && cp ../../README.sdk.md ./README.md && cp ../../.npmrc ./.npmrc && npm run package-publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torderdev/boilerplate-test.git"
  },
  "author": "Dongsoo Lee <lds1@torder.co.kr>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/torderdev/boilerplate-test/issues"
  },
  "homepage": "https://github.com/torderdev/boilerplate-test#readme",
  "dependencies": {
    "@nestia/core": "2.4.4",
    "@nestjs/axios": "3.0.1",
    "@nestjs/common": "10.3.0",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.3.0",
    "@nestjs/mongoose": "10.0.2",
    "@nestjs/platform-express": "10.3.0",
    "@nestjs/schedule": "4.0.0",
    "@nestjs/swagger": "^7.1.17",
    "axios": "1.6.5",
    "camelcase-keys": "7.0.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "express": "4.18.2",
    "fp-ts": "2.16.2",
    "jsonwebtoken": "^9.0.2",
    "kysely": "0.27.2",
    "moment": "2.30.1",
    "mongoose": "8.0.4",
    "mysql2": "3.7.0",
    "nestia": "5.1.2",
    "node-fetch": "3.3.2",
    "snakecase-keys": "5.5.0",
    "tsconfig-paths": "4.2.0",
    "typeorm": "0.3.19",
    "typia": "5.4.4"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.23",
    "@nestia/e2e": "0.4.0",
    "@nestia/sdk": "2.4.4",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "20.10.8",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/eslint-plugin-tslint": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unicorn": "50.0.1",
    "jest": "^29.7.0",
    "nodemon": "3.0.2",
    "prettier": "3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-patch": "3.1.2",
    "typescript": "5.3.2",
    "typescript-transform-paths": "3.4.6"
  }
}
